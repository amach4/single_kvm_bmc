[DEFAULT]
log_config_append = {{ infoblox_ipam_agent_logging_config_file }}


# Infoblox-ipam-agent errors out on the Ardana rpc_backend setting for neutron.
# Since it doesn't make RPC calls.  So redefine the "rpc_backend" back to
# neutron's default value.
rpc_backend = rabbit


